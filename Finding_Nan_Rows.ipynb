    "\n",
    "def findnanrows(df):\n",
    "    \n",
    "    is_NaN = df.isnull()\n",
    "    row_has_NaN = is_NaN.any(axis=1)\n",
    "    rows_with_NaN = df[row_has_NaN]\n",
    "    return rows_with_NaN"
   ]
  }
